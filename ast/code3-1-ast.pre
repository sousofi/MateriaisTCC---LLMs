Module(
    body=[
        Expr(
            value=Call(
                func=Name(id='print', ctx=Load()),
                args=[
                    Constant(value='\nBem-vindo à Central de Ingressos do Play Center')],
                keywords=[])),
        Assign(
            targets=[
                Name(id='nome', ctx=Store())],
            value=Call(
                func=Name(id='input', ctx=Load()),
                args=[
                    Constant(value='Nome: ')],
                keywords=[])),
        Assign(
            targets=[
                Name(id='idade', ctx=Store())],
            value=Call(
                func=Name(id='int', ctx=Load()),
                args=[
                    Call(
                        func=Name(id='input', ctx=Load()),
                        args=[
                            Constant(value='Idade: ')],
                        keywords=[])],
                keywords=[])),
        Assign(
            targets=[
                Name(id='altura', ctx=Store())],
            value=Call(
                func=Name(id='float', ctx=Load()),
                args=[
                    Call(
                        func=Name(id='input', ctx=Load()),
                        args=[
                            Constant(value='Altura: ')],
                        keywords=[])],
                keywords=[])),
        If(
            test=BoolOp(
                op=Or(),
                values=[
                    Compare(
                        left=Name(id='idade', ctx=Load()),
                        ops=[
                            Lt()],
                        comparators=[
                            Constant(value=4)]),
                    Compare(
                        left=Name(id='idade', ctx=Load()),
                        ops=[
                            GtE()],
                        comparators=[
                            Constant(value=65)])]),
            body=[
                Assign(
                    targets=[
                        Name(id='ingresso', ctx=Store())],
                    value=Constant(value=0.0)),
                Assign(
                    targets=[
                        Name(id='tipo_acesso', ctx=Store())],
                    value=Constant(value='SEM ACESSO'))],
            orelse=[
                If(
                    test=Compare(
                        left=Name(id='idade', ctx=Load()),
                        ops=[
                            Lt()],
                        comparators=[
                            Constant(value=13)]),
                    body=[
                        Assign(
                            targets=[
                                Name(id='ingresso', ctx=Store())],
                            value=Constant(value=75.0))],
                    orelse=[
                        If(
                            test=Compare(
                                left=Name(id='idade', ctx=Load()),
                                ops=[
                                    Lt()],
                                comparators=[
                                    Constant(value=18)]),
                            body=[
                                Assign(
                                    targets=[
                                        Name(id='ingresso', ctx=Store())],
                                    value=Constant(value=85.0))],
                            orelse=[
                                If(
                                    test=Compare(
                                        left=Name(id='idade', ctx=Load()),
                                        ops=[
                                            GtE()],
                                        comparators=[
                                            Constant(value=18)]),
                                    body=[
                                        Assign(
                                            targets=[
                                                Name(id='ingresso', ctx=Store())],
                                            value=Constant(value=115.0))],
                                    orelse=[])])])]),
        If(
            test=Compare(
                left=Name(id='altura', ctx=Load()),
                ops=[
                    Lt()],
                comparators=[
                    Constant(value=1.5)]),
            body=[
                Assign(
                    targets=[
                        Name(id='ingresso', ctx=Store())],
                    value=BinOp(
                        left=Name(id='ingresso', ctx=Load()),
                        op=Mult(),
                        right=Constant(value=0.9)))],
            orelse=[]),
        If(
            test=Compare(
                left=Name(id='altura', ctx=Load()),
                ops=[
                    Lt()],
                comparators=[
                    Constant(value=1.5)]),
            body=[
                Assign(
                    targets=[
                        Name(id='tipo_acesso', ctx=Store())],
                    value=Constant(value='ACESSO RESTRITO'))],
            orelse=[
                Assign(
                    targets=[
                        Name(id='tipo_acesso', ctx=Store())],
                    value=Constant(value='ACESSO TOTAL'))]),
        Expr(
            value=Call(
                func=Name(id='print', ctx=Load()),
                args=[
                    JoinedStr(
                        values=[
                            FormattedValue(
                                value=Name(id='nome', ctx=Load()),
                                conversion=-1),
                            Constant(value=', seu ingresso é do tipo'),
                            FormattedValue(
                                value=Name(id='tipo_acesso', ctx=Load()),
                                conversion=-1),
                            Constant(value=' e o valor é R$'),
                            FormattedValue(
                                value=Name(id='ingresso', ctx=Load()),
                                conversion=-1),
                            Constant(value='.')])],
                keywords=[]))],
    type_ignores=[])
